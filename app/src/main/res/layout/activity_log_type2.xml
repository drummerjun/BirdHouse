<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/general_setting_layout"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".LogTypeActivity"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <include layout="@layout/toolbar"/>
    <RelativeLayout android:background="@drawable/selector_header_background"
        android:layout_width="wrap_content"
        android:layout_height="52dp"
        android:padding="@dimen/activity_horizontal_margin">
        <TextView android:id="@+id/header_shadow"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="1dp"
            android:layout_marginStart="1dp"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Small"/>
        <TextView android:id="@+id/header_date"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Small.Inverse"
            android:textColor="@android:color/white"/>
    </RelativeLayout>
    <ScrollView android:fillViewport="true"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">
        <LinearLayout android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content">
                <RelativeLayout android:id="@+id/sw_button_1"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:background="@drawable/selector_plain_bg_inverse">
                    <ImageView android:id="@+id/sw_report"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_marginStart="@dimen/activity_horizontal_margin"
                        android:layout_marginEnd="@dimen/activity_horizontal_margin"
                        android:layout_marginTop="@dimen/activity_vertical_margin"
                        android:layout_marginBottom="@dimen/activity_vertical_margin"
                        android:layout_alignParentStart="true"
                        android:layout_centerVertical="true"
                        android:scaleType="fitCenter"
                        android:adjustViewBounds="true"/>
                    <RelativeLayout android:id="@+id/button_text1"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginTop="@dimen/activity_vertical_margin"
                        android:layout_marginBottom="@dimen/activity_vertical_margin"
                        android:layout_toEndOf="@id/sw_report">
                        <TextView android:text="@string/log_sw"
                            android:textColor="@drawable/selector_button_text2"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginTop="1dp"
                            android:layout_marginStart="1dp"
                            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                            android:gravity="center_vertical|start"/>
                        <TextView android:text="@string/log_sw"
                            android:textColor="@drawable/selector_button_text1"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:textAppearance="@style/TextAppearance.AppCompat.Medium.Inverse"
                            android:gravity="center_vertical|start"/>
                    </RelativeLayout>
                </RelativeLayout>
                <View android:layout_height="1dp" android:layout_width="match_parent"/>
                <RelativeLayout android:id="@+id/sw_button_2"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:background="@drawable/selector_plain_bg_inverse">
                    <ImageView android:id="@+id/hourly_report"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_marginStart="@dimen/activity_horizontal_margin"
                        android:layout_marginEnd="@dimen/activity_horizontal_margin"
                        android:layout_marginTop="@dimen/activity_vertical_margin"
                        android:layout_marginBottom="@dimen/activity_vertical_margin"
                        android:layout_alignParentStart="true"
                        android:layout_centerVertical="true"
                        android:scaleType="fitCenter"
                        android:adjustViewBounds="true" />
                    <RelativeLayout android:id="@+id/button_text2"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginTop="@dimen/activity_vertical_margin"
                        android:layout_marginBottom="@dimen/activity_vertical_margin"
                        android:layout_toEndOf="@id/hourly_report">
                        <TextView android:text="@string/log_info"
                            android:textColor="@drawable/selector_button_text2"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginTop="1dp"
                            android:layout_marginStart="1dp"
                            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                            android:gravity="center_vertical|start"/>
                        <TextView android:text="@string/log_info"
                            android:textColor="@drawable/selector_button_text1"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:textAppearance="@style/TextAppearance.AppCompat.Medium.Inverse"
                            android:gravity="center_vertical|start"/>
                    </RelativeLayout>
                </RelativeLayout>
                <View android:layout_height="1dp" android:layout_width="match_parent"/>
                <RelativeLayout android:id="@+id/sw_button_3"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:background="@drawable/selector_plain_bg_inverse">
                    <ImageView android:id="@+id/entry_report"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_marginStart="@dimen/activity_horizontal_margin"
                        android:layout_marginEnd="@dimen/activity_horizontal_margin"
                        android:layout_marginTop="@dimen/activity_vertical_margin"
                        android:layout_marginBottom="@dimen/activity_vertical_margin"
                        android:layout_alignParentStart="true"
                        android:layout_centerVertical="true"
                        android:scaleType="fitCenter"
                        android:adjustViewBounds="true"/>
                    <RelativeLayout android:id="@+id/button_text3"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginTop="@dimen/activity_vertical_margin"
                        android:layout_marginBottom="@dimen/activity_vertical_margin"
                        android:layout_toEndOf="@id/entry_report">
                        <TextView android:text="@string/log_entries"
                            android:textColor="@drawable/selector_button_text2"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginTop="1dp"
                            android:layout_marginStart="1dp"
                            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                            android:gravity="center_vertical|start"/>
                        <TextView android:text="@string/log_entries"
                            android:textColor="@drawable/selector_button_text1"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:textAppearance="@style/TextAppearance.AppCompat.Medium.Inverse"
                            android:gravity="center_vertical|start"/>
                    </RelativeLayout>
                </RelativeLayout>
            </LinearLayout>
            <View android:layout_width="1dp"
                android:layout_height="match_parent"/>
            <LinearLayout android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content">
                <RelativeLayout android:id="@+id/sw_button_4"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:background="@drawable/selector_plain_bg_inverse">
                    <ImageView android:id="@+id/intrusion_report"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_marginStart="@dimen/activity_horizontal_margin"
                        android:layout_marginEnd="@dimen/activity_horizontal_margin"
                        android:layout_marginTop="@dimen/activity_vertical_margin"
                        android:layout_marginBottom="@dimen/activity_vertical_margin"
                        android:layout_alignParentStart="true"
                        android:layout_centerVertical="true"
                        android:scaleType="fitCenter"
                        android:adjustViewBounds="true"/>
                    <RelativeLayout android:id="@+id/button_text4"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginTop="@dimen/activity_vertical_margin"
                        android:layout_marginBottom="@dimen/activity_vertical_margin"
                        android:layout_toEndOf="@id/intrusion_report">
                        <TextView android:text="@string/log_intrusion"
                            android:textColor="@drawable/selector_button_text2"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginTop="1dp"
                            android:layout_marginStart="1dp"
                            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                            android:gravity="center_vertical|start"/>
                        <TextView android:text="@string/log_intrusion"
                            android:textColor="@drawable/selector_button_text1"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:textAppearance="@style/TextAppearance.AppCompat.Medium.Inverse"
                            android:gravity="center_vertical|start"/>
                    </RelativeLayout>
                </RelativeLayout>
                <View android:layout_height="1dp" android:layout_width="match_parent"/>
                <RelativeLayout android:id="@+id/sw_button_5"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:background="@drawable/selector_plain_bg_inverse">
                    <ImageView android:id="@+id/power_report"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_marginStart="@dimen/activity_horizontal_margin"
                        android:layout_marginEnd="@dimen/activity_horizontal_margin"
                        android:layout_marginTop="@dimen/activity_vertical_margin"
                        android:layout_marginBottom="@dimen/activity_vertical_margin"
                        android:layout_alignParentStart="true"
                        android:layout_centerVertical="true"
                        android:scaleType="fitCenter"
                        android:adjustViewBounds="true"/>
                    <RelativeLayout android:id="@+id/button_text5"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginTop="@dimen/activity_vertical_margin"
                        android:layout_marginBottom="@dimen/activity_vertical_margin"
                        android:layout_toEndOf="@id/power_report">
                        <TextView android:text="@string/log_power"
                            android:textColor="@drawable/selector_button_text2"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginTop="1dp"
                            android:layout_marginStart="1dp"
                            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                            android:gravity="center_vertical|start"/>
                        <TextView android:text="@string/log_power"
                            android:textColor="@drawable/selector_button_text1"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:textAppearance="@style/TextAppearance.AppCompat.Medium.Inverse"
                            android:gravity="center_vertical|start"/>
                    </RelativeLayout>
                </RelativeLayout>
                <View android:layout_height="1dp" android:layout_width="match_parent"/>
                <RelativeLayout android:id="@+id/sw_button_6"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:background="@drawable/selector_plain_bg_inverse">
                    <ImageView android:id="@+id/count_report"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_marginStart="@dimen/activity_horizontal_margin"
                        android:layout_marginEnd="@dimen/activity_horizontal_margin"
                        android:layout_marginTop="@dimen/activity_vertical_margin"
                        android:layout_marginBottom="@dimen/activity_vertical_margin"
                        android:layout_alignParentStart="true"
                        android:layout_centerVertical="true"
                        android:scaleType="fitCenter"
                        android:adjustViewBounds="true"/>
                    <RelativeLayout android:id="@+id/button_text6"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginTop="@dimen/activity_vertical_margin"
                        android:layout_marginBottom="@dimen/activity_vertical_margin"
                        android:layout_toEndOf="@id/count_report">
                        <TextView android:text="@string/log_count"
                            android:textColor="@drawable/selector_button_text2"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginTop="1dp"
                            android:layout_marginStart="1dp"
                            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                            android:gravity="center_vertical|start"/>
                        <TextView android:text="@string/log_count"
                            android:textColor="@drawable/selector_button_text1"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:textAppearance="@style/TextAppearance.AppCompat.Medium.Inverse"
                            android:gravity="center_vertical|start"/>
                    </RelativeLayout>
                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
    <include layout="@layout/bottom_controls"/>
</LinearLayout>

